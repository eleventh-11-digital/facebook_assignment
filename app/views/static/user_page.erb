<h1><%=@uu%></h1>
<a style="margin-left: 70%" href='/home'><button>Back</button></a>

<div>
	<% @status.all.each do |s| %>
		<a href="/status<%=s.id%>"><%= s.status %></a>

		<form method=post action='/likes/<%= s.id %>/user' value="Likes">
			<button type="submit">Likes
				<span><%= s.likes.where(likes: 1).count %></span>
			</button>
		</form>
<%end%>